<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="neosoft.jp.pojo.News" table="news">
		<!-- 主键 -->
		<id name="id" type="java.lang.Integer" column="id">
				<!-- 主键生成器
				<generator class="sequence">
					<param name="sequence">CUSTOMER_SEQ</param>
				</generator>  -->
		</id>
		<property name="insert_time"  type="java.lang.String" column="insert_time"/>
		<property name="insert_text"  type="java.lang.String" column="insert_text"/>
	</class>
</hibernate-mapping>