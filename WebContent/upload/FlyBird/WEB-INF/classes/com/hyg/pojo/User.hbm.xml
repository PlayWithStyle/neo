<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hyg.pojo.User" table="customer">
		<!-- 主键 -->
		<id name="userid" type="java.lang.Integer" column="userid">
				<!-- 主键生成器 -->
				<generator class="sequence">
					<param name="sequence">CUSTOMER_SEQ</param>
				</generator>
		</id>
		<property name="username"  type="java.lang.String" column="username"/>
		<property name="password"  type="java.lang.String" column="password"/>
		<property name="age"       type="java.lang.String" column="age"/>
		<property name="sex" 	   type="java.lang.String" column="sex"/>
		<property name="tall"      type="java.lang.String" column="tall"/>
		<property name="address"   type="java.lang.String" column="address"/>
		<property name="education" type="java.lang.String" column="education"/>
		<property name="money"     type="java.lang.String" column="money"/>
		<property name="privilege" type="java.lang.String" column="privilege"/>
		<property name="photo"     type="java.lang.String" column="photo"/>
		<property name="nature"    type="java.lang.String" column="nature"/>
		<property name="other"     type="java.lang.String" column="other"/>
	</class>
</hibernate-mapping>